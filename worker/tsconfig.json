{
  "compilerOptions": {
    "target": "es2022",
    "lib": ["ES2022"],
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "node16",
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "outDir": "./build/"
  },
  "ts-node": {
      "compilerOptions": {
          "module": "node16"
      }
  }
}
